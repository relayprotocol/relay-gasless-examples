# Relay Enterprise API key — required for /execute endpoint
# Must have a sponsoringWalletAddress configured
VITE_RELAY_API_KEY=your-relay-api-key

# Pimlico RPC URL — required for ERC-4337 paymaster + bundler
# Get one at https://dashboard.pimlico.io
VITE_PIMLICO_RPC_URL=https://api.pimlico.io/v2/8453/rpc?apikey=your-pimlico-key

# Private key for EIP-7702 flow — required because browser wallets cannot
# sign standalone 7702 authorizations (no wallet_signAuthorization RPC standard exists).
# Use a test wallet with ETH on the origin chain.
VITE_EOA_PRIVATE_KEY=0xyour-private-key

# WalletConnect project ID — get one at https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID=your-project-id
